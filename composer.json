{
    "type": "symfony-bundle",
    "name": "kuznetsovsite/math",
    "description": "Math plugin",
    "license": "proprietary",
    "require": {
        "php": "^7.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sk\\Math\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sk\\Math\\Tests\\": "tests/"
        }
    }
}
