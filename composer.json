{
    "type": "symfony-bundle",
    "name": "kuznetsovsite/math",
    "description": "Math plugin",
    "license": "proprietary",
    "require": {
        "php": "^7.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "5.0.*",
        "chriskonnertz/string-calc": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sk\\MathBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sk\\MathBundle\\Tests\\": "Tests/"
        }
    }
}
